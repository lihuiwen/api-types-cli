{
  "name": "api-types-cli",
  "version": "0.0.1",
  "description": "🚀 API 接口 TypeScript 类型生成器",
  "type": "module",
  "bin": {
    "api-types": "./bin/index.js"
  },
  "main": "./bin/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "start": "node bin/index.js",
    "lint": "eslint src/**/*.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:integration": "jest --testNamePattern='集成测试|性能测试'",
    "test:github": "jest --testNamePattern='GitHub'",
    "build": "tsc",
    "build:test": "tsc -p tsconfig.test.json --noEmit",
    "test": "jest",
    "test:compile": "tsc -p tsconfig.test.json --noEmit",
    "test:unit": "jest --testNamePattern='基础功能测试|配置文件测试|错误处理测试'",
    "test:quick": "jest --testNamePattern='应该能创建.*实例'",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "keywords": [
    "api",
    "typescript",
    "types",
    "generator",
    "cli",
    "quicktype",
    "json"
  ],
  "author": "lhw",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "fs-extra": "^11.1.1",
    "quicktype-core": "^23.0.75",
    "axios": "^1.6.0",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/fs-extra": "^11.0.2",
    "@types/inquirer": "^9.0.6",
    "@types/js-yaml": "^4.0.6",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0"
  },
  "files": [
    "README.md",
    "bin"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}